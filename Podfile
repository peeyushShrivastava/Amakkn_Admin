def common_pods

  pod 'Localize-Swift'
  pod 'SDWebImage'
  pod 'SwiftHash'
  pod 'Socket.IO-Client-Swift'
  pod 'Firebase/Messaging'

end

target 'AmakknCustmerSupport' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  common_pods

  # Pods for AmakknCustmerSupport

  target 'AmakknCustmerSupportTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AmakknCustmerSupportUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
